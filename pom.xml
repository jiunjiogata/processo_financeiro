		<springboot.version>2.4.2</springboot.version>
		<liquibase.version>4.0.0</liquibase.version>
		<postgres.driver.version>42.2.19</postgres.driver.version>
		<javax.persistance.version>1.0.2</javax.persistance.version>
		<hikari.version>3.2.0</hikari.version>
		<javax.version>3.1.0</javax.version>
		<jackson.version>2.9.8</jackson.version>
		<logback.version>1.2.1</logback.version>

	</properties>

	<dependencies>

	</dependencies>

